{
  "name": "hubboardmap",
  "version": "0.2.0",
  "description": "A node server which should handle HTTP request from user, then response and CRUD the datum to mongoDB via mongoose and express. This server should also sending request to external API to retrieve data.",
  "main": "server.js",
  "scripts": {
    "start": "react-scripts build && serve -s build -l 80",
    "server-dev": "nodemon server.js",
    "dev": "concurrently --names \"CLIENT,SERVER\" \"npm run start\" \"npm run server-dev\"",
    "build": "react-scripts build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "CSCI2720 group12",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.2.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mongoose": "^6.8.0",
    "nodemon": "^2.0.20",
    "papaparse": "^5.3.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "serve": "^14.1.2",
    "xml2json": "^0.12.0"
  },
  "overrides": {
    "@svgr/plugin-svgo": {
      "svgo": "3.0.2"
    },
    "xml2json": {
      "joi": "17.7.0"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
